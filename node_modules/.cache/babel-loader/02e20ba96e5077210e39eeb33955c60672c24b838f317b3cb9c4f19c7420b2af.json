{"ast":null,"code":"import { fetchOrders, updateOrder, deleteOrder } from '@/services/orderService';\nexport default {\n  data() {\n    return {\n      orders: [],\n      filteredOrders: [],\n      searchQuery: '',\n      showEditModal: false,\n      selectedOrder: null\n    };\n  },\n  methods: {\n    async fetchOrders() {\n      try {\n        this.orders = await fetchOrders();\n        this.filteredOrders = this.orders;\n      } catch (error) {\n        console.error('Error fetching orders:', error);\n      }\n    },\n    searchOrders() {\n      this.filteredOrders = this.orders.filter(order => {\n        return order.customer.name.toLowerCase().includes(this.searchQuery.toLowerCase()) || order.status.toLowerCase().includes(this.searchQuery.toLowerCase());\n      });\n    },\n    editOrder(order) {\n      this.selectedOrder = order;\n      this.showEditModal = true;\n    },\n    closeEditModal() {\n      this.showEditModal = false;\n      this.selectedOrder = null;\n    },\n    async updateOrder() {\n      try {\n        await updateOrder(this.selectedOrder);\n        await this.fetchOrders();\n        this.closeEditModal();\n      } catch (error) {\n        console.error('Error updating order:', error);\n      }\n    },\n    async deleteOrder(orderId) {\n      try {\n        await deleteOrder(orderId);\n        await this.fetchOrders();\n      } catch (error) {\n        console.error('Error deleting order:', error);\n      }\n    }\n  },\n  mounted() {\n    this.fetchOrders();\n  }\n};","map":{"version":3,"names":["fetchOrders","updateOrder","deleteOrder","data","orders","filteredOrders","searchQuery","showEditModal","selectedOrder","methods","error","console","searchOrders","filter","order","customer","name","toLowerCase","includes","status","editOrder","closeEditModal","orderId","mounted"],"sources":["C:\\Users\\Rupert\\Desktop\\Capstone2024_Front-End\\src\\views\\adminviews\\OrderManagement.vue"],"sourcesContent":["<template>\r\n  <div class=\"order-management\">\r\n    <h1>Order Management</h1>\r\n    <div class=\"filter-search-section\">\r\n      <input v-model=\"searchQuery\" @input=\"searchOrders\" placeholder=\"Search orders...\" />\r\n    </div>\r\n    <table>\r\n      <thead>\r\n      <tr>\r\n        <th>Order ID</th>\r\n        <th>Customer</th>\r\n        <th>Total Price</th>\r\n        <th>Status</th>\r\n        <th>Actions</th>\r\n      </tr>\r\n      </thead>\r\n      <tbody>\r\n      <tr v-for=\"order in filteredOrders\" :key=\"order.id\">\r\n        <td>{{ order.id }}</td>\r\n        <td>{{ order.customer.name }}</td>\r\n        <td>{{ order.totalPrice }}</td>\r\n        <td>{{ order.status }}</td>\r\n        <td>\r\n          <button @click=\"editOrder(order)\">Edit</button>\r\n          <button @click=\"deleteOrder(order.id)\">Delete</button>\r\n        </td>\r\n      </tr>\r\n      </tbody>\r\n    </table>\r\n    <div v-if=\"showEditModal\" class=\"modal\">\r\n      <div class=\"modal-content\">\r\n        <h2>Edit Order</h2>\r\n        <!-- Form for editing order -->\r\n        <button @click=\"closeEditModal\">Close</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { fetchOrders, updateOrder, deleteOrder } from '@/services/orderService';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      orders: [],\r\n      filteredOrders: [],\r\n      searchQuery: '',\r\n      showEditModal: false,\r\n      selectedOrder: null,\r\n    };\r\n  },\r\n  methods: {\r\n    async fetchOrders() {\r\n      try {\r\n        this.orders = await fetchOrders();\r\n        this.filteredOrders = this.orders;\r\n      } catch (error) {\r\n        console.error('Error fetching orders:', error);\r\n      }\r\n    },\r\n    searchOrders() {\r\n      this.filteredOrders = this.orders.filter(order => {\r\n        return order.customer.name.toLowerCase().includes(this.searchQuery.toLowerCase()) ||\r\n            order.status.toLowerCase().includes(this.searchQuery.toLowerCase());\r\n      });\r\n    },\r\n    editOrder(order) {\r\n      this.selectedOrder = order;\r\n      this.showEditModal = true;\r\n    },\r\n    closeEditModal() {\r\n      this.showEditModal = false;\r\n      this.selectedOrder = null;\r\n    },\r\n    async updateOrder() {\r\n      try {\r\n        await updateOrder(this.selectedOrder);\r\n        await this.fetchOrders();\r\n        this.closeEditModal();\r\n      } catch (error) {\r\n        console.error('Error updating order:', error);\r\n      }\r\n    },\r\n    async deleteOrder(orderId) {\r\n      try {\r\n        await deleteOrder(orderId);\r\n        await this.fetchOrders();\r\n      } catch (error) {\r\n        console.error('Error deleting order:', error);\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.fetchOrders();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.order-management {\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n}\r\n\r\n.filter-search-section {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.filter-search-section input {\r\n  padding: 5px;\r\n}\r\n\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\n\r\nth, td {\r\n  text-align: left;\r\n  padding: 10px;\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\nbutton {\r\n  padding: 5px 10px;\r\n  background-color: #3498db;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #2980b9;\r\n}\r\n\r\n.modal {\r\n  display: block;\r\n  position: fixed;\r\n  z-index: 1000;\r\n  left: 0;\r\n  top: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  overflow: auto;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n}\r\n\r\n.modal-content {\r\n  background-color: #fff;\r\n  margin: 10% auto;\r\n  padding: 20px;\r\n  border: 1px solid #888;\r\n  width: 50%;\r\n}\r\n</style>"],"mappings":"AAwCA,SAASA,WAAW,EAAEC,WAAW,EAAEC,WAAU,QAAS,yBAAyB;AAE/E,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,cAAc,EAAE,EAAE;MAClBC,WAAW,EAAE,EAAE