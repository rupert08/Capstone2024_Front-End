rror);\r\n      }\r\n    },\r\n    async checkUserStatus() {\r\n      try {\r\n        const userDetails = await getUserDetails();\r\n        this.user = userDetails;\r\n        this.isLoggedIn = true;\r\n      } catch (error) {\r\n        this.isLoggedIn = false;\r\n      }\r\n    },\r\n    async logout() {\r\n      try {\r\n        await logoutUser();\r\n        this.isLoggedIn = false;\r\n        this.user = { name: '', surname: '', role: '' };\r\n      } catch (error) {\r\n        console.error('Failed to logout:', error);\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    homeRoute() {\r\n      if (this.isLoggedIn) {\r\n        return this.user.role === 'admin' ? '/admin' : '/home';\r\n      }\r\n      return '/';\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.app-container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  min-height: 100vh;\r\n}\r\n\r\n.content {\r\n  flex: 1;\r\n  padding: 20px;\r\n}\r\n\r\n.nav-wrapper {\r\n  padding: 0 20px;\r\n  background-color: #333;\r\n}\r\n\r\n.nav-wrapper .left,\r\n.nav-wrapper .right {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\nul#nav-mobile.left {\r\n  margin: 0;\r\n  padding: 0;\r\n  flex: 1;\r\n}\r\n\r\n.brand-logo {\r\n  color: white;\r\n  flex: 2;\r\n  text-align: center;\r\n}\r\n\r\nul#nav-mobile li {\r\n  display: flex;\r\n  align-items: center;\r\n  margin: 0 10px;\r\n}\r\n\r\n.material-icons {\r\n  vertical-align: middle;\r\n  color: white;\r\n}\r\n\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-top: 20px;\r\n}\r\n\r\nth, td {\r\n  text-align: left;\r\n  padding: 10px;\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\n.footer {\r\n  text-align: center;\r\n  padding: 10px 20px;\r\n  background-color: #333;\r\n  color: #fff;\r\n  margin-top: 20px;\r\n  border-radius: 10px;\r\n  position: static;\r\n  bottom: 0;\r\n  width: 100%;\r\n  max-height: 150px;\r\n  overflow: auto;\r\n}\r\n</style>"],"mappings":"AA0CA,OAAOA,UAAS,MAAO,yBAAyB;AAChD,SAASC,SAAQ,QAAS,yBAA