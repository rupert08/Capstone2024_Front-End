n          })\r\n          .catch(error => {\r\n            console.error('Error updating order:', error);\r\n          });\r\n    },\r\n    deleteOrder(orderID) {\r\n      deleteOrder(orderID)\r\n          .then(() => {\r\n            this.fetchOrders();\r\n          })\r\n          .catch(error => {\r\n            console.error('Error deleting order:', error);\r\n          });\r\n    },\r\n    closeModal() {\r\n      this.showModal = false;\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchOrders();\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Add your styles here */\r\n</style>"],"mappings":"AAiFA,SAASA,SAAS,EAAEC,WAAW,EAAEC,WAAW,EAAEC,WAAU,QAAS,yBAAyB;AAE1F,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE,KAAK;MAChBC,UAAU,EAAE,KAAK;MACjBC,SAAS,EAAE;QACTC,OAAO,EAAE,EAAE;QACXC,SAAS,EAAE,EAAE;QACbC,WAAW,EAAE,EAAE;QACfC,WAAW,EAAE,EAAE;QACfC,kBAAkB,EAAE,KAAK;QACzBC,IAAI,EAAE;UACJC,MAAM,EAAE;QACV,CAAC;QACDC,QAAQ,EAAE;UACRC,UAAU,EAAE;QACd,CAAC;QACDC,aAAa,EAAE;UACbC,MAAM,EAAE;QACV;MACF;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZrB,SAAS,CAAC,EACLsB,IAAI,CAAClB,IAAG,IAAK;QACZ,IAAI,CAACC,MAAK,GAAID,IAAI;QAClBmB,OAAO,CAACC,GAAG,CAAC,iBAAiB,EAAE,IAAI,CAACnB,MAAM,CAAC,EAAE;MAC/C,CAAC,EACAoB,KAAK,CAACC,KAAI,IAAK;QACdH,OAAO,CAACG,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;MAChD,CAAC,CAAC;IACR,CAAC;IACDC,oBAAoBA,CAAA,EAAG;MACrB,IAAI,CAACpB,UAAS,GAAI,KAAK;MACvB,IAAI,CAACC,SAAQ,GAAI;QACfC,OAAO,EAAE,EAAE;QACXC,SAAS,EAAE,EAAE;QACbC,WAAW,EAAE,EAAE;QACfC,WAAW,EAAE,EAAE;QACfC,kBAAkB,EAAE,KAAK;QACzBC,IAAI,EAAE;UACJC,MAAM,EAAE;QACV,CAAC;QACDC,QAAQ,EAAE;UACRC,UAAU,EAAE;QACd,CAAC;QACDC,aAAa,EAAE;UACbC,MAAM,EAAE;QACV;MACF,CAAC;MACD,IAAI,CAACb,SAAQ,GAAI,IAAI;IACvB,CAAC;IACDsB,kBAAkBA,CAACC,KAAK,EAAE;MACxB,IAAI,CAACtB,UAAS,GAAI,IAAI;MACtB,IAAI,CAACC,SAAQ,GAAI;QAAE,GAAGqB;MAAM,CAAC;MAC7B,IAAI,CAACvB,SAAQ,GAAI,IAAI;IACvB,CAAC;IACDL,WAAWA,CAAA,EAAG;MACZA,WAAW,CAAC,IAAI,CAACO,SAAS,EACrBc,IAAI,CAAC,MAAM;QACV,IAAI,CAACD,WAAW,CAAC,CAAC;QAClB,IAAI,CAACS,U