{"ast":null,"code":"import AdminSidebar from './AdminSidebar.vue';\nimport ProductManagement from './ProductManagement.vue';\nimport OrderManagement from './OrderManagement.vue';\nimport RecentCustomer from '@/components/RecentCustomer.vue';\nimport RecentCategories from '@/components/RecentCategories.vue';\nimport { getOrders } from '@/services/orderService';\nexport default {\n  components: {\n    AdminSidebar,\n    ProductManagement,\n    OrderManagement,\n    RecentCategories,\n    RecentCustomer\n  },\n  data() {\n    return {\n      recentOrders: [],\n      totalOrders: 0,\n      totalRevenue: 0.00\n    };\n  },\n  async created() {\n    await this.fetchOrders();\n  },\n  methods: {\n    async fetchOrders() {\n      try {\n        const orders = await getOrders();\n        this.recentOrders = orders.slice(-3).reverse();\n        this.totalOrders = orders.length;\n        this.totalRevenue = orders.reduce((sum, order) => sum + order.totalAmount, 0);\n      } catch (error) {\n        console.error('Error fetching orders:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["AdminSidebar","ProductManagement","OrderManagement","RecentCustomer","RecentCategories","getOrders","components","data","recentOrders","totalOrders","totalRevenue","created","fetchOrders","methods","orders","slice","reverse","length","reduce","sum","order","totalAmount","error","console"],"sources":["C:\\Users\\Rupert\\Desktop\\Capstone2024_Front-End\\src\\views\\adminviews\\AdminDashboard.vue"],"sourcesContent":["<template>\r\n  <div id=\"dashboard\">\r\n    <AdminSidebar />\r\n    <div class=\"content\">\r\n      <div class=\"header\">\r\n        <h1>Admin Dashboard</h1>\r\n        <input type=\"text\" placeholder=\"Search...\" class=\"search-bar\" />\r\n      </div>\r\n      <div class=\"metrics\">\r\n        <!-- Add metrics display here -->\r\n        <h2>Metrics/KPIs</h2>\r\n        <p>Total Orders: {{ totalOrders }}</p>\r\n        <p>Total Revenue: {{ totalRevenue }}</p>\r\n      </div>\r\n      <div class=\"recent-orders\">\r\n        <!-- Add recent orders display here -->\r\n        <h2>Recent Orders</h2>\r\n        <table>\r\n          <thead>\r\n            <tr>\r\n              <th>Order ID</th>\r\n              <th>Order Date</th>\r\n              <th>Order Status</th>\r\n              <th>Total Amount</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"order in recentOrders\" :key=\"order.id\">\r\n              <td>{{ order.id }}</td>\r\n              <td>{{ order.orderDate }}</td>\r\n              <td>{{ order.orderStatus }}</td>\r\n              <td>{{ order.totalAmount }}</td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <RecentCustomer />\r\n      <ProductManagement />\r\n      <RecentCategories />\r\n      <OrderManagement />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AdminSidebar from './AdminSidebar.vue';\r\nimport ProductManagement from './ProductManagement.vue';\r\nimport OrderManagement from './OrderManagement.vue';\r\nimport RecentCustomer from '@/components/RecentCustomer.vue';\r\nimport RecentCategories from '@/components/RecentCategories.vue';\r\nimport { getOrders } from '@/services/orderService';\r\n\r\nexport default {\r\n  components: {\r\n    AdminSidebar,\r\n    ProductManagement,\r\n    OrderManagement,\r\n    RecentCategories,\r\n    RecentCustomer\r\n  },\r\n  data() {\r\n    return {\r\n      recentOrders: [],\r\n      totalOrders: 0,\r\n      totalRevenue: 0.00\r\n    };\r\n  },\r\n  async created() {\r\n    await this.fetchOrders();\r\n  },\r\n  methods: {\r\n    async fetchOrders() {\r\n      try {\r\n        const orders = await getOrders();\r\n        this.recentOrders = orders.slice(-3).reverse();\r\n        this.totalOrders = orders.length;\r\n        this.totalRevenue = orders.reduce((sum, order) => sum + order.totalAmount, 0);\r\n      } catch (error) {\r\n        console.error('Error fetching orders:', error);\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#dashboard {\r\n  display: flex;\r\n  padding: 20px;\r\n}\r\n.content {\r\n  flex-grow: 1;\r\n  padding: 20px;\r\n  background-color: #f4f7fc;\r\n}\r\n\r\n.header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.search-bar {\r\n  padding: 5px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n}\r\n\r\n.metrics, .recent-orders {\r\n  margin-bottom: 20px;\r\n}\r\n\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-top: 20px;\r\n}\r\n\r\nth, td {\r\n  text-align: left;\r\n  padding: 10px;\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n</style>"],"mappings":"AA6CA,OAAOA,YAAW,MAAO,oBAAoB;AAC7C,OAAOC,iBAAgB,MAAO,yBAAyB;AACvD,OAAOC,eAAc,MAAO,uBAAuB;AACnD,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,gBAAe,MAAO,mCAAmC;AAChE,SAASC,SAAQ,QAAS,yBAAyB;AAEnD,eAAe;EACbC,UAAU,EAAE;IACVN,YAAY;IACZC,iBAAiB;IACjBC,eAAe;IACfE,gBAAgB;IAChBD;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,EAAE;MAChBC,WAAW,EAAE,CAAC;MACdC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,WAAW,CAAC,CAAC;EAC1B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,WAAWA,CAAA,EAAG;MAClB,IAAI;QACF,MAAME,MAAK,GAAI,MAAMT,SAAS,CAAC,CAAC;QAChC,IAAI,CAACG,YAAW,GAAIM,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC,CAAC,CAACC,OAAO,CAAC,CAAC;QAC9C,IAAI,CAACP,WAAU,GAAIK,MAAM,CAACG,MAAM;QAChC,IAAI,CAACP,YAAW,GAAII,MAAM,CAACI,MAAM,CAAC,CAACC,GAAG,EAAEC,KAAK,KAAKD,GAAE,GAAIC,KAAK,CAACC,WAAW,EAAE,CAAC,CAAC;MAC/E,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;MAChD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}