{"ast":null,"code":"import PageFooter from './components/Footer.vue';\nimport { getUserDetails, logoutUser } from '@/services/userService';\nexport default {\n  name: 'App',\n  components: {\n    PageFooter\n  },\n  data() {\n    return {\n      cartCount: 0,\n      cartValue: 0.00,\n      isLoggedIn: false,\n      user: {\n        name: '',\n        surname: '',\n        role: '' // Add role to user data\n      }\n    };\n  },\n  mounted() {\n    this.fetchCartData();\n    this.checkUserStatus();\n  },\n  methods: {\n    fetchCartData() {\n      setTimeout(() => {\n        this.cartCount = 0;\n        this.cartValue = 0.00;\n      }, 1000);\n    },\n    async checkUserStatus() {\n      try {\n        const userDetails = await getUserDetails();\n        this.user = userDetails;\n        this.isLoggedIn = true;\n      } catch (error) {\n        this.isLoggedIn = false;\n      }\n    },\n    async logout() {\n      try {\n        await logoutUser();\n        this.isLoggedIn = false;\n        this.user = {\n          name: '',\n          surname: '',\n          role: ''\n        };\n      } catch (error) {\n        console.error('Failed to logout:', error);\n      }\n    }\n  },\n  computed: {\n    homeRoute() {\n      if (this.isLoggedIn) {\n        return this.user.role === 'admin' ? '/admin' : '/home';\n      }\n      return '/';\n    }\n  }\n};","map":{"version":3,"names":["PageFooter","getUserDetails","logoutUser","name","components","data","cartCount","cartValue","isLoggedIn","user","surname","role","mounted","fetchCartData","checkUserStatus","methods","setTimeout","userDetails","error","logout","console","computed","homeRoute"],"sources":["C:\\Users\\Rupert\\Desktop\\Capstone2024_Front-End\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\" class=\"app-container\">\r\n    <nav>\r\n      <div class=\"nav-wrapper\">\r\n        <ul id=\"nav-mobile\" class=\"left\">\r\n          <li>\r\n            <router-link :to=\"homeRoute\">\r\n              <i class=\"material-icons\">home</i>\r\n            </router-link>\r\n          </li>\r\n        </ul>\r\n        <a href=\"#\" class=\"brand-logo\">Hardware Store</a>\r\n        <ul id=\"nav-mobile\" class=\"right\"></ul>\r\n      </div>\r\n    </nav>\r\n    <div class=\"content\">\r\n      <router-view />\r\n    </div>\r\n    <PageFooter />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport PageFooter from './components/Footer.vue';\r\nimport { getUserDetails, logoutUser } from '@/services/userService';\r\n\r\nexport default {\r\n  name: 'App',\r\n  components: {\r\n    PageFooter\r\n  },\r\n  data() {\r\n    return {\r\n      cartCount: 0,\r\n      cartValue: 0.00,\r\n      isLoggedIn: false,\r\n      user: {\r\n        name: '',\r\n        surname: '',\r\n        role: '' // Add role to user data\r\n      }\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchCartData();\r\n    this.checkUserStatus();\r\n  },\r\n  methods: {\r\n    fetchCartData() {\r\n      setTimeout(() => {\r\n        this.cartCount = 0;\r\n        this.cartValue = 0.00;\r\n      }, 1000);\r\n    },\r\n    async checkUserStatus() {\r\n      try {\r\n        const userDetails = await getUserDetails();\r\n        this.user = userDetails;\r\n        this.isLoggedIn = true;\r\n      } catch (error) {\r\n        this.isLoggedIn = false;\r\n      }\r\n    },\r\n    async logout() {\r\n      try {\r\n        await logoutUser();\r\n        this.isLoggedIn = false;\r\n        this.user = { name: '', surname: '', role: '' };\r\n      } catch (error) {\r\n        console.error('Failed to logout:', error);\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    homeRoute() {\r\n      if (this.isLoggedIn) {\r\n        return this.user.role === 'admin' ? '/admin' : '/home';\r\n      }\r\n      return '/';\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.app-container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  min-height: 100vh; /* Ensure full viewport height */\r\n}\r\n\r\n.content {\r\n  flex: 